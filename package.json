{
  "name": "@bitblit/ratchet",
  "version": "0.12.3",
  "description": "Common tools for general use",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "directories": [
    "dist"
  ],
  "contributors": [
    "Christopher Weiss <bitblit@gmail.com>",
    "William Weiss <npm@codification.org>",
    "Austin Grantham <agrantham@algidious.com>"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 140,
    "singleQuote": true,
    "arrowParens": "always"
  },
  "config": {},
  "scripts": {
    "prepublishOnly": "yarn run clean-compile",
    "clean-compile": "yarn run clean && yarn run compile",
    "compile": "tsc",
    "watch": "tsc-watch",
    "clean": "shx rm -Rf dist",
    "compile-test": "yarn compile && yarn test",
    "test": "jest",
    "apply-circle": "node dist/circleci/apply-circle-ci-env-variables-to-files src/static/build-properties.json",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitblit/Ratchet"
  },
  "keywords": [
    "wrench",
    "utility"
  ],
  "bugs": {
    "url": "https://github.com/bitblit/Ratchet/issues"
  },
  "homepage": "https://github.com/bitblit/Ratchet#readme",
  "engines": {
    "node": ">=14.15"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "moment-timezone": "0.5.33",
    "ts-enum-util": "4.0.2",
    "yargs": "16.2.0"
  },
  "resolutions": {
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "1.190.0",
    "@types/aws-lambda": "8.10.72",
    "@types/node": "14.14.31",
    "@types/yargs": "15.0.12",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "aws-sdk": "2.852.0",
    "csv": "5.3.2",
    "eslint": "7.20.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "handlebars": "4.7.7",
    "handlebars-layouts": "3.1.4",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "portable-fetch": "3.0.0",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "rxjs": "6.6.6",
    "shx": "0.3.3",
    "tmp": "0.2.1",
    "ts-jest": "26.5.2",
    "ts-node": "9.1.1",
    "tsc-watch": "4.2.9",
    "typescript": "4.2.2",
    "walk": "2.3.14"
  }
}
