{
  "name": "@bitblit/ratchet",
  "version": "0.10.1",
  "description": "Common tools for general use",
  "main": "./dist/index",
  "typings": "./dist/index",
  "files": [
    "dist"
  ],
  "contributors": [
    "Christopher Weiss <bitblit@gmail.com>",
    "William Weiss <npm@codification.org>",
    "Austin Grantham <agrantham@algidious.com>"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 140,
    "singleQuote": true,
    "arrowParens": "always"
  },
  "config": {},
  "scripts": {
    "prepublishOnly": "yarn run compile",
    "compile": "yarn run clean && tsc",
    "watch": "tsc-watch",
    "clean": "shx rm -Rf dist",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "apply-circle": "node dist/circleci/apply-circle-ci-env-variables-to-files src/static/build-properties.json",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitblit/Ratchet"
  },
  "keywords": [
    "wrench",
    "utility"
  ],
  "bugs": {
    "url": "https://github.com/bitblit/Ratchet/issues"
  },
  "homepage": "https://github.com/bitblit/Ratchet#readme",
  "engines": {
    "node": ">=12.8"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "moment": "2.29.1",
    "moment-timezone": "0.5.32",
    "ts-enum-util": "4.0.2",
    "yargs": "16.2.0"
  },
  "resolutions": {
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.70",
    "@types/chai": "4.2.14",
    "@types/mocha": "8.2.0",
    "@types/node": "12.12.6",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "@types/yargs": "15.0.12",
    "aws-sdk": "2.824.0",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "csv": "5.3.2",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "handlebars": "4.7.6",
    "handlebars-layouts": "3.1.4",
    "husky": "4.3.7",
    "mocha": "8.2.1",
    "portable-fetch": "3.0.0",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "rxjs": "6.6.3",
    "shx": "0.3.3",
    "tmp": "0.2.1",
    "ts-node": "9.1.1",
    "tsc-watch": "4.2.9",
    "typescript": "3.9.7",
    "walk": "2.3.14"
  }
}
