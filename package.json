{
  "name": "@bitblit/ratchet",
  "version": "0.7.2",
  "description": "Common tools for general use",
  "main": "./dist/index",
  "typings": "./dist/index",
  "files": [
    "dist"
  ],
  "contributors": [
    "Christopher Weiss <bitblit@gmail.com>",
    "William Weiss <npm@codification.org>"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 140,
    "singleQuote": true,
    "arrowParens": "always"
  },
  "config": {},
  "scripts": {
    "prepublishOnly": "yarn run compile",
    "compile": "yarn run clean && tsc",
    "watch": "tsc-watch",
    "clean": "shx rm -Rf dist",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "apply-circle": "node dist/circleci/apply-circle-ci-env-variables-to-files src/static/build-properties.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitblit/Ratchet"
  },
  "keywords": [
    "wrench",
    "utility"
  ],
  "bugs": {
    "url": "https://github.com/bitblit/Ratchet/issues"
  },
  "homepage": "https://github.com/bitblit/Ratchet#readme",
  "engines": {
    "node": ">=6.10.2"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "moment": "2.25.3",
    "moment-timezone": "0.5.28",
    "yargs": "14.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.31",
    "@types/chai": "4.2.1",
    "@types/mocha": "5.2.7",
    "@types/node": "10.12.18",
    "aws-sdk": "2.524.0",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "csv": "5.1.2",
    "handlebars": "4.7.6",
    "handlebars-layouts": "3.1.4",
    "mocha": "6.2.0",
    "tsc-watch": "4.2.3",
    "portable-fetch": "3.0.0",
    "rxjs": "6.5.3",
    "shx": "0.3.2",
    "tmp": "0.1.0",
    "ts-node": "8.3.0",
    "tslint": "5.19.0",
    "typescript": "3.6.2",
    "walk": "2.3.14",
    "husky": "3.0.5",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1"
  }
}
