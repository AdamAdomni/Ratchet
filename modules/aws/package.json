{
  "name": "@bitblit/ratchet-aws",
  "version": "0.0.0",
  "description": "Common tools for use with AWS browser and node",
  "note-on-side-effects": "Technically the entries in 'bin' below might be side effects, but they are called explicitly",
  "sideEffects": false,
  "bin": {
    "ratchet-start-instance-and-ssh": "./cli-bootstrap/ratchet-start-instance-and-ssh.js",
    "ratchet-apply-ci-env-variables-to-files": "./cli-bootstrap/ratchet-apply-ci-env-variables-to-files.js",
    "ratchet-publish-ci-release-to-slack": "./cli-bootstrap/ratchet-publish-ci-release-to-slack.js",
    "ratchet-files-to-static-class": "./cli-bootstrap/ratchet-files-to-static-class.js",
    "ratchet-site-uploader": "./cli-bootstrap/ratchet-site-uploader.js"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/*"
  ],
  "contributors": [
    "Christopher Weiss <bitblit@gmail.com>",
    "William Weiss <npm@codification.org>",
    "Austin Grantham <agrantham@algidious.com>",
    "Lucas Myers <veganbeef@protonmail.com>",
    "Joel Flint <joel@stimulimedia.net>",
    "Bilal Shahid <bshahid@adomni.com>"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 140,
    "singleQuote": true,
    "arrowParens": "always"
  },
  "config": {},
  "scripts": {
    "prepare-to-publish": "yarn abuild && yarn run apply-ci-vars && yarn remove-compiled-tests",
    "watch": "tsc-watch",
    "remove-compiled-tests": "rimraf ./dist/**/*.spec.d.ts && rimraf ./dist/**/*.spec.js && rimraf ./dist/**/*.spec.js.map",
    "clean": "shx rm -Rf dist",
    "compile-test": "yarn compile && yarn test",
    "apply-ci-vars": "node dist/cjs/node-only/ci/apply-ci-env-variables-to-files github dist/cjs/build/ratchet-info.js && node dist/cjs/node-only/ci/apply-ci-env-variables-to-files github dist/es/build/ratchet-info.js",
    "test": "jest",
    "docs": "typedoc",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "generate-barrels": "barrelsby -q --delete -d src -e .*\\.spec\\.ts",

    "build": "yarn run clean && yarn run generate-barrels && concurrently 'yarn:build:cjs' 'yarn:build:es' 'yarn:build:types'",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:es": "tsc -p tsconfig.es.json",
    "build:types": "tsc -p tsconfig.types.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitblit/Ratchet"
  },
  "keywords": [
    "wrench",
    "utility"
  ],
  "bugs": {
    "url": "https://github.com/bitblit/Ratchet/issues"
  },
  "homepage": "https://github.com/bitblit/Ratchet#readme",
  "engines": {
    "node": ">=14.18"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@bitblit/ratchet-common": "*",
    "@aws-sdk/types": "3.272.0",
    "@aws-sdk/abort-controller": "3.272.0",
    "@aws-sdk/client-athena": "3.272.0",
    "@aws-sdk/client-batch": "3.272.0",
    "@aws-sdk/client-cloudwatch": "3.272.0",
    "@aws-sdk/client-cloudwatch-logs": "3.272.0",
    "@aws-sdk/client-dynamodb": "3.272.0",
    "@aws-sdk/client-ec2": "3.272.0",
    "@aws-sdk/client-ec2-instance-connect": "3.272.0",
    "@aws-sdk/client-route-53": "3.272.0",
    "@aws-sdk/client-s3": "3.272.0",
    "@aws-sdk/client-ses": "3.272.0",
    "@aws-sdk/client-sns": "3.272.0",
    "@aws-sdk/client-ssm": "3.272.0",
    "@aws-sdk/lib-dynamodb": "3.272.0",
    "@aws-sdk/lib-storage": "3.272.0",
    "@aws-sdk/s3-request-presigner": "3.272.0",
    "@aws-sdk/smithy-client": "3.272.0",
    "@aws-sdk/util-waiter": "3.272.0",
    "@types/aws-lambda": "8.10.111"
  },
  "peerDependencies": {
    "@bitblit/ratchet-jest": "*",
    "@aws-sdk/abort-controller": "^3.272.0",
    "@aws-sdk/client-athena": "^3.272.0",
    "@aws-sdk/client-batch": "^3.272.0",
    "@aws-sdk/client-cloudwatch": "^3.272.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.272.0",
    "@aws-sdk/client-dynamodb": "^3.272.0",
    "@aws-sdk/client-ec2": "^3.272.0",
    "@aws-sdk/client-ec2-instance-connect": "^3.272.0",
    "@aws-sdk/client-route-53": "^3.272.0",
    "@aws-sdk/client-s3": "^3.272.0",
    "@aws-sdk/client-ses": "^3.272.0",
    "@aws-sdk/client-sns": "^3.272.0",
    "@aws-sdk/client-ssm": "^3.272.0",
    "@aws-sdk/lib-dynamodb": "^3.272.0",
    "@aws-sdk/s3-request-presigner": "^3.272.0",
    "@aws-sdk/smithy-client": "^3.272.0",
    "@aws-sdk/types": "^3.272.0",
    "@aws-sdk/util-waiter": "^3.272.0",
    "@types/aws-lambda": "^8.10.111"
  },
  "resolutions": {
  },
  "devDependencies": {
    "aws-sdk-client-mock": "2.0.1",
    "@bitblit/ratchet-jest": "*"
  }
}
