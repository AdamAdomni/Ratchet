{
  "name": "@bitblit/ratchet-echarts",
  "version": "0.0.0",
  "description": "Library for using echarts on Node",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "contributors": [
    "Christopher Weiss <bitblit@gmail.com>"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 140,
    "singleQuote": true,
    "arrowParens": "always"
  },
  "config": {},
  "scripts": {
    "prepare-to-publish": "yarn build && yarn remove-compiled-tests && yarn run apply-ci-vars",
    "copy-static-files": "shx cp -R src/static dist",
    "watch": "tsc-watch",
    "remove-compiled-tests": "rimraf ./dist/examples && rimraf ./dist/**/*.spec.d.ts && rimraf ./dist/**/*.spec.js && rimraf ./dist/**/*.spec.js.map",
    "clean": "shx rm -Rf dist",
    "compile-test": "yarn compile && yarn test",
    "apply-ci-vars": "yarn ratchet-apply-ci-env-variables-to-files github dist/static/build-properties.json",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "generate-barrels": "barrelsby -q --delete -d src -l top -e .*\\.spec\\.ts",
    "build": "yarn run clean && yarn run generate-barrels && concurrently 'yarn:build:cjs' 'yarn:build:es' 'yarn:build:types'",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:es": "tsc -p tsconfig.es.json",
    "build:types": "tsc -p tsconfig.types.json"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "canvas": "2.11.0",
    "echarts": "5.4.1"
  },
  "devDependencies": {
    "@bitblit/ratchet-common": "*",
    "@types/jest": "29.2.6",
    "@types/node": "14.18.1",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "barrelsby": "2.5.1",
    "cross-fetch": "3.1.5",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "jest": "29.3.1",
    "jsonwebtoken": "8.5.1",
    "prettier": "2.8.3",
    "pretty-quick": "3.1.3",
    "rimraf": "4.1.2",
    "shx": "0.3.4",
    "ts-jest": "29.0.5",
    "typescript": "4.6.4"
  }
}
